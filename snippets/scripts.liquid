<script>
  AOS.init();
  $(document).ready(function() {
    l(),
    r(),
    AOS.init({
      once: !0
    }),
    $("[name=purchase_type]").click(function() {
      this.value == "onetime"
        ? ($(".onetime").show(), $(".autodeliver").hide())
        : ($(".onetime").hide(), $(".autodeliver").show())
    })
  });
  function n(e, t) {
    t || (t = window.location.href),
    e = e.replace(/[\[\]]/g, "\\$&");
    var i = new RegExp("[?&]" + e + "(=([^&#]*)|&|#|$)"),
      o = i.exec(t);
    return o
      ? o[2]
        ? decodeURIComponent(o[2].replace(/\+/g, " "))
        : ""
      : null
  }
  if ($(window).on("load", function() {
    console.log("window loaded"),
    AOS.init({
      once: !0
    }),
    n("scroll") && $("html, body").animate({
      scrollTop: $("#reviews").offset().top
    }, 1500),
    n("customer_posted") && $("html, body").animate({
      scrollTop: $("footer").offset().top
    }, 1500)
  }), $(".box-cotent-single-blog").length && $.each($(".box-cotent-single-blog p"), function() {
    $(this).find("img").length && $(this).addClass("p-with-img")
  }), $(".section-product-benefics").length) {
    var d = $(".section-product-benefics").offset().top + 250,
      m = $("footer").offset().top - 490;
    $(window).scroll(function(e) {
      var t = $(this).scrollTop();
      t > d
        ? (
          $(".box-submit-fixed-product2").addClass("box-submit-fixed-product"),
          $(".section-producto").addClass("height500"),
          $(".box-submit-fixed-product2").addClass("active"),
          t > m
            ? $(".box-submit-fixed-product2").addClass("pause")
            : $(".box-submit-fixed-product2").removeClass("pause")
        )
        : ($(".box-submit-fixed-product2").removeClass("active"), $(".box-submit-fixed-product2").removeClass("pause"), $(".box-submit-fixed-product2").removeClass("box-submit-fixed-product"), $(".section-producto").removeClass("height500"))
    })
  }
</script>

